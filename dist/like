fucntion like(storyID) {
	
	$.post('php/like_processing.php', {storyID:storyID}, function(data){

		if(data == 'success'){

			fetchlikes(storyID);

		}
		else{

		alert(data);

		}
	});
}

function fetchlikes(storyID){
	$.post('fetch_likes.php',{storyID:storyID}, function(data){
		$('likecounter').text(data);

	})

}